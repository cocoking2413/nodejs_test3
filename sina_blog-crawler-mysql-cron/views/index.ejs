<!doctype>
<html>
<head>
    <title>test cat news</title>
    <link href="/css/zui.css" rel="stylesheet">
    <script src="/lib/jquery/jquery.js" type="test/javascript"></script>
    <script src="/js/zui.js" type="test/javascript"></script>
</head>
<body>
<div id="body" class="container-fluid">
    <div class="row-fluid">
        <div id="title" class="col-md-offset-1 col-md-10"><h1>我的博客首页</h1></div>
    </div>
    <div class="row-fluid">
        <div class="col-md-offset-1 col-md-2" style="border: solid 1px #c2b1e6">
            <ul class="ul nav" id="nav">
                <% classlist.forEach(function (cla) { %>
                <li><a href="/class/<%= cla.id %>"><%= cla.name %></a></li>
                <% }) %>
            </ul>
        </div>
        <div class="col-md-7" style="border: solid 1px #5c99fa">
            <ul class="ul nav">
                <% articleList.forEach(function (article) { %>
                <li><a href="/article/<%= article.id %>"><%= article.title %></a></li>
                <% }) %>
            </ul>
        </div>
    </div>
</div>
</body>
</html>